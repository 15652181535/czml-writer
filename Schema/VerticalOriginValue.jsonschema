{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://cesiumjs.org/schemas/czml/VerticalOriginValue.jsonschema",
    "title": "VerticalOrigin",
    "description": "A vertical origin, which specifies whether an element is bottom-, center-, or top-aligned with respect to an origin.  Valid values are \"BOTTOM\", \"CENTER\", and \"TOP\".",
    "type": "string",
    "enum": [
        "BOTTOM",
        "CENTER",
        "TOP"
    ]
}